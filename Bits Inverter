using System;

class BitsInverter
{
    static void Main()
    {
        byte n = byte.Parse(Console.ReadLine());
        byte step = byte.Parse(Console.ReadLine());

        byte[] numbersArr = new byte[n];

        byte mask = 1;
        int bitToInvert = 0;
        int byteCounter = 8;
        int bitCounter = 0;
        int counter = 0;

        for (int i = 0; i < n; i++)
        {
            numbersArr[i] = byte.Parse(Console.ReadLine());
        }

        for (int i = 0; i < numbersArr.Length; i++)
        {
            for (int y = bitCounter; ; y++)
            {
                bitToInvert = (byteCounter - (1 + (y * step)));

                if (bitToInvert < byteCounter&&bitToInvert>=0)
                {
                    mask = (byte)(mask << bitToInvert);

                    numbersArr[i] = (byte)(numbersArr[i] ^ mask);
                    mask = (byte)(mask >> bitToInvert);
                    counter += 1;
                }
                else
                {
                    break;
                }
            }
            bitCounter = counter;
            byteCounter += 8;
        }

        for (int i = 0; i < numbersArr.Length; i++)
        {
            Console.WriteLine(numbersArr[i]);
        }
    }
}

//5_Bits Inverter 
//C# Basics Exam 10 April 2014 Morning
