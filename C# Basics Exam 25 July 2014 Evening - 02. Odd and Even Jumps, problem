using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Text.RegularExpressions;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {
            string letters = Console.ReadLine();
            letters = letters.ToLower();
            int oddJump = int.Parse(Console.ReadLine());
            int evenJump = int.Parse(Console.ReadLine());
            
            letters = Regex.Replace(letters, "\\s+", "");
            List<int> odd = new List<int>();
            List<int> even = new List<int>();
            ulong resultodd = 0;
            ulong resulteven = 0;
            for (int i = 0, o = 0, e = 0; i < letters.Length; i++)
            {
                if ((i + 1) % 2 == 0)
                {
                    even.Add(letters[i]);
                    e++;
                }
                else
                {
                    odd.Add(letters[i]);
                    o++;
                }
            }
            for (int o = 0, e = 1; o < odd.Count; o++, e++)
            {

                if (e % oddJump == 0)
                {
                    resultodd *= (ulong)odd[o];
                    o++;
                    e++;
                }
                if (o == odd.Count)
                {
                    break;
                }
                resultodd += (ulong)odd[o];
            }
            for (int o = 0, e = 1; o < even.Count; o++, e++)
            {

                if (e % evenJump == 0)
                {
                    resulteven *= (ulong)even[o];
                    o++;
                    e++;
                }
                if (o == even.Count)
                {
                    break;
                }
                resulteven += (ulong)even[o];
            }   

                Console.WriteLine("Odd: {0:X}", resultodd);
                Console.WriteLine("Even: {0:X}", resulteven);
            
        }
    }
}
